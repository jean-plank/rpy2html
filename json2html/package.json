{
  "name": "json2html",
  "version": "1.0.0",
  "description": "Web visual novel game engine using a json.",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "~test": "jest --watchAll",
    "dc:graph": "depcruise --exclude '(^node_modules|\\.css\\.d\\.ts$|^test|\\.css$)' --output-type dot . | dot -T svg > dependencygraph.svg",
    "dc:osage": "depcruise --exclude '(^node_modules|\\.css\\.d\\.ts$|^test|\\.css$)' --output-type dot . | osage -Gpack=16 -Gsplines=ortho -GpackMode=array2 -T svg > dependencygraph.svg",
    "dc:check": "depcruise --validate .dependency-cruiser.js ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Grimalkin8675/rpy2html.git"
  },
  "author": "Grimalkin8675",
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.7",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "acorn": "^6.1.1",
    "ajv": "^6.10.0",
    "autoprefixer": "^9.6.0",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^1.0.1",
    "file-loader": "^3.0.1",
    "fp-ts": "^1.18.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "io-ts": "^1.8.6",
    "lodash": "^4.17.11",
    "markdown-loader": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "redux": "^4.0.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "typescript": "^3.5.1",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.13",
    "dependency-cruiser": "^4.21.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.1.1",
    "webpack-dev-server": "^3.7.1"
  }
}
