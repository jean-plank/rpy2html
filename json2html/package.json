{
  "name": "json2html",
  "version": "1.0.0",
  "description": "Web visual novel game engine using a json.",
  "scripts": {
    "~compile": "tsc -w --pretty false",
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "~test": "jest --watchAll",
    "dc:graph": "depcruise --exclude '(^node_modules|^dist|^test|\\.d\\.ts$|context\\.ts|styles\\.ts)' --output-type dot . | dot -T svg > dependencygraph.svg",
    "dc:osage": "depcruise --exclude '(^node_modules|^dist|^test|\\.d\\.ts$|context\\.ts|styles\\.ts)' --output-type dot . | osage -Gpack=16 -Gsplines=ortho -GpackMode=array2 -T svg > dependencygraph.svg",
    "dc:check": "depcruise --validate .dependency-cruiser.js ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Grimalkin8675/rpy2html.git"
  },
  "author": "Grimalkin8675",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.12",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "ajv": "^6.10.0",
    "awesome-typescript-loader": "^5.2.1",
    "file-loader": "^3.0.1",
    "fp-ts": "^1.19.1",
    "fp-ts-contrib": "^0.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "io-ts": "^1.8.6",
    "markdown-loader": "^5.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.13",
    "dependency-cruiser": "^4.21.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.1.1",
    "webpack-dev-server": "^3.7.1"
  }
}
