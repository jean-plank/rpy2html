{
  "name": "json2html",
  "version": "1.2.0",
  "description": "Web visual novel game engine using a json.",
  "homepage": "https://github.com/jean-plank/rpy2html/tree/master/json2html",
  "scripts": {
    "~compile": "tsc -w --pretty false",
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "loader:start": "tsc -w -p renpy-json-loader",
    "loader:compile": "tsc -p renpy-json-loader",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "~test": "jest --watchAll",
    "dc:graph": "depcruise --exclude '(^node_modules|^dist|^test|\\.d\\.ts$|context\\.ts)' --output-type dot . | dot -T svg > dependencygraph.svg",
    "dc:osage": "depcruise --exclude '(^node_modules|^dist|^test|\\.d\\.ts$|context\\.ts)' --output-type dot . | osage -Gpack=16 -Gsplines=ortho -GpackMode=array2 -T svg > dependencygraph.svg",
    "dc:check": "depcruise --validate .dependency-cruiser.js ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jean-plank/rpy2html.git"
  },
  "author": "Gurimarukin",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-transition-group": "^4.2.3",
    "@types/webpack": "^4.39.8",
    "awesome-typescript-loader": "^5.2.1",
    "file-loader": "^4.2.0",
    "fp-ts": "^2.1.2",
    "fp-ts-contrib": "^0.1.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "io-ts": "^2.0.1",
    "io-ts-types": "^0.5.2",
    "markdown-loader": "^5.1.0",
    "monocle-ts": "^2.0.0",
    "newtype-ts": "^0.3.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-transition-group": "^4.3.0",
    "source-map-loader": "^0.2.4",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.23",
    "dependency-cruiser": "^5.3.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "tslint-react-hooks": "^2.2.1",
    "webpack-dev-server": "^3.9.0"
  }
}
